{
  "expo": {
    "name": "Blocks",
    "slug": "Blocks",
    "version": "1.1.26",
    "scheme": "blocks",
    "platforms": [
      "ios",
      "android"
    ],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/blocks-icon-128px.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      "expo-secure-store",
      "expo-font",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow this app to use Face ID to sign you in securely."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/blocks-icon-128px.png",
          "color": "#ffffff",
          "sounds": []
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos so you can upload images.",
          "cameraPermission": "The app needs access to your camera so you can take photos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/blocks-icon-128px.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/blocks-icon-128px.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.intelik.Blocks",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSFaceIDUsageDescription": "Allow this app to use Face ID to sign you in securely."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyDNmbakkExapYfPx7hwQZU3ajVsC-xamAs"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/blocks-icon-128px.png",
        "backgroundColor": "#000000"
      },
      "package": "com.intelik.Blocks",
      "googleServicesFile": "./google-services.json", 
      "versionCode": 137,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDNmbakkExapYfPx7hwQZU3ajVsC-xamAs"
        }
      }
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ead54695-f47e-4652-809f-4d7992799c28"
      },
      "apiUrl": "https://blocks-backend.vercel.app"
    },
    "owner": "robas",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ead54695-f47e-4652-809f-4d7992799c28"
    }
  }
}
