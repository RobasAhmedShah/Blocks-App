{
  "expo": {
    "name": "Blocks",
    "slug": "Blocks",
    "version": "1.0.0",
    "scheme": "blocks",
    "platforms": [
      "ios",
      "android"
    ],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      "expo-secure-store",
      "expo-font",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow this app to use Face ID to sign you in securely."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "sounds": []
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.intelik.Blocks",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "blocks",
              "com.googleusercontent.apps.452166186757-qelam6ha1qtarj3vjm0aiv9lu667r76g"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.intelik.Blocks",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "blocks"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ead54695-f47e-4652-809f-4d7992799c28"
      },
      "apiUrl": "https://blocks-backend.vercel.app",
      "googleAndroidClientId": "452166186757-unrelq5fmdrkd06dqeb0huks2b3fjeve.apps.googleusercontent.com",
      "googleIosClientId": "452166186757-qelam6ha1qtarj3vjm0aiv9lu667r76g.apps.googleusercontent.com",
      "magicPublishableKey": "pk_live_9E93488C0CC96A3B"
    },
    "owner": "robas",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ead54695-f47e-4652-809f-4d7992799c28"
    }
  }
}
